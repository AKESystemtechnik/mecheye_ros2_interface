<?xml version="1.0"?>
<launch>
    <arg name="save_file" default="false"/>
    <arg name="camera_ip" default="192.168.0.123"/>
    <arg name="use_external_intri" default="false"/>
    <arg name="fx" default="1727.4641025602748"/>
    <arg name="fy" default="1727.4586926701952"/>
    <arg name="u" default="655.8180825729554"/>
    <arg name="v" default="516.6306500606158"/>

    <node name="mechmind_camera_publisher_service" pkg="mecheye_ros_interface" exec="start" output="screen">
        <param name="save_file" value="$(var save_file)"/>
        <param name="camera_ip" value="$(var camera_ip)"/>
        <param name="use_external_intri" value="$(var use_external_intri)"/>
        <param name="fx" value="$(var fx)"/>
        <param name="fy" value="$(var fy)"/>
        <param name="u" value="$(var u)"/>
        <param name="v" value="$(var v)"/>
    </node>
</launch>